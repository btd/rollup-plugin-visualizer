    "@types/d3-scale": "^3.2.2",
    "@types/d3-shape": "^2.0.0",
    "@types/nanoid": "^2.1.0",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "bytes": "^3.1.0",
    "d3-array": "^2.12.1",
    "d3-color": "^2.0.0",
    "d3-hierarchy": "^2.0.0",
    "d3-scale": "^3.3.0",
    "d3-shape": "^2.1.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
