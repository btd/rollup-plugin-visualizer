    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "postcss-url": "^10.1.3",
    "preact": "^10.5.13",
    "prettier": "^2.2.1",
    "rollup": "^2.46.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.12",
    "typescript": "^4.2.4",
    "webcola": "^3.4.0"
  },
  "engines": {
    "node": ">=10.16"
  },
