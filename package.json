{
  "name": "rollup-plugin-visualizer",
  "version": "1.1.1",
  "main": "plugin.js",
  "author": "Denis Bardadym <bardadymchik@gmail.com>",
  "license": "MIT",
  "files": [
    "lib/*",
    "plugin.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:btd/rollup-plugin-visualizer.git"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "cp ./src/*.css ./lib/ && rollup -c rollup.config.js -i ./src/script-sunburst.js -o ./lib/main-sunburst.js && rollup -c rollup.config.js -i ./src/script-treemap.js -o ./lib/main-treemap.js",
    "build:dev": "cp ./src/*.css ./lib/ && rollup -c rollup.config-dev.js -i ./src/script-sunburst.js -o ./lib/main-sunburst.js && rollup -c rollup.config-dev.js -i ./src/script-treemap.js -o ./lib/main-treemap.js",
    "prepare": "yarn run build",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "dependencies": {
    "d3-collection": "^1.0.7",
    "mkdirp": "^0.5.1",
    "open": "^6.3.0",
    "source-map": "^0.7.3",
    "typeface-oswald": "0.0.54"
  },
  "peerDependencies": {
    "rollup": ">=0.60.0"
  },
  "devDependencies": {
    "bytes": "^3.1.0",
    "d3-hierarchy": "^1.1.8",
    "d3-scale": "^3.0.0",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.4",
    "eslint": "^5.14.1",
    "prettier": "^1.16.4",
    "rollup": "^1.14.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "engines": {
    "node": ">=8.10"
  }
}
